    "MEDIA_STORAGE_CONNECTION_STRING": "DefaultEndpointsProtocol=https;AccountName=<account>;AccountKey=<key>;EndpointSuffix=core.windows.net",
    "MEDIA_CONTAINER_NAME": "datebook-media"
